{
  "name": "react-tiptap",
  "version": "0.0.1",
  "description": "React tiptap rich text editor",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./globals.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://react-tiptap.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/OgabekYuldoshev/react-tiptap.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check": "pnpm biome check --write src"
  },
  "keywords": [
    "react",
    "react-tiptap",
    "react-editor",
    "tiptap",
    "rich-text-editor",
    "rich-text-editor-react"
  ],
  "author": "Ogabek Yuldoshev <https://yuldashoff.uz/>",
  "license": "ISC",
  "peerDependencies": {
    "@tiptap/react": "^2.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tiptap/react": "^2.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.447.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  }
}
