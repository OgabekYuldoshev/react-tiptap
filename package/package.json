{
  "name": "react-tiptap",
  "version": "0.0.0",
  "description": "React tiptap rich text editor",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./base.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://react-tiptap.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/OgabekYuldoshev/react-tiptap.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check": "pnpm biome check --write src",
    "size": "size-limit"
  },
  "keywords": [
    "react",
    "react-tiptap",
    "react-editor",
    "tiptap",
    "rich-text-editor",
    "rich-text-editor-react"
  ],
  "author": "Ogabek Yuldoshev <https://yuldashoff.uz/>",
  "license": "ISC",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "500 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "500 KB"
    }
  ],
  "peerDependencies": {
    "@tiptap/core": "^2.8.0",
    "@tiptap/react": "^2.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "@tiptap/core": "^2.8.0",
    "@tiptap/react": "^2.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "esbuild-style-plugin": "^1.6.3",
    "sass": "^1.79.4",
    "size-limit": "^11.1.6",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.447.0"
  }
}
